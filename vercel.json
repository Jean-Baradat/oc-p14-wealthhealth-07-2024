{
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	],
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"buildCommand": {
					"production": "npm run build:prod",
					"preview": ""
				},
				"outputDirectory": "dist"
			}
		}
	]
}
